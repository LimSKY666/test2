<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="TOP_LEFT" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.test2.WeatherController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TextField fx:id="city"/>
    <Button text="Search!" onAction="#onHelloButtonClick"/>
    <Label fx:id="name"/>
    <Label fx:id="temp"/>
    <Label fx:id="tempMax"/>
    <Label fx:id="tempMin"/>
    <Label fx:id="feelsLike"/>
    <Label fx:id="description"/>

</VBox>
